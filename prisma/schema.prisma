generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Motobike {
  id           Int      @id @default(autoincrement())
  name         String   @db.VarChar(255)
  brand_id     String?
  cc           Int?
  type         String?
  transmission String?
  price        Int?
  created_at   DateTime @default(now())
  updated_at   DateTime @default(now()) @updatedAt
}
